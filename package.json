{
	"name": "opm_frontend",
	"version": "1.0.0",
	"description": "try webpack",
	"main": "index.js",
	"scripts": {
		"start": "webpack-dev-server --mode development --open --hot --host",
		"build": "webpack --mode production",
		"clean": "webpack --clean",
		"test": "jest --coverage --coverageDirectory=output/coverage/jest"
	},
	"author": "shield",
	"license": "ISC",
	"dependencies": {
		"ag-grid-enterprise": "21.2.2",
		"ag-grid-community": "^21.2.1",
		"ag-grid-react": "^21.2.1",
		"autoprefixer": "^9.6.1",
		"axios": "^0.19.0",
		"babel-core": "^6.26.3",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"bootstrap": "^4.3.1",
		"crypto-js": "^3.1.9-1",
		"css-loader": "^3.2.0",
		"formsy-react-es6": "^1.0.7",
		"is-jpg": "^2.0.0",
		"jquery": "^3.4.1",
		"js-cookie": "^2.2.1",
		"popper.js": "^1.15.0",
		"postcss-loader": "^3.0.0",
		"precss": "^4.0.0",
		"react": "^16.10.1",
		"react-activity": "^1.2.2",
		"react-bootstrap": "^1.0.0-beta.11",
		"react-bootstrap-date-picker": "^5.1.0",
		"react-bootstrap-tabs": "^1.0.2",
		"react-datepicker": "^2.9.6",
		"react-dom": "^16.9.0",
		"react-redux": "^7.1.0",
		"react-responsive-carousel": "^3.1.50",
		"react-router": "^5.0.1",
		"react-router-dom": "^5.0.1",
		"react-scripts": "^3.1.1",
		"react-step-progress-bar": "^1.0.3",
		"react-switch": "5.0.1",
		"react-tabs": "^3.0.0",
		"redux": "^4.0.4",
		"redux-thunk": "^2.3.0",
		"sass-loader": "^7.2.0",
		"save-dev": "https://registry.npmjs.org/save-dev/-/save-dev-0.0.1-security.tgz",
		"sinon": "^7.4.2",
		"style-loader": "^1.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.5.5",
		"@babel/preset-env": "^7.5.5",
		"@babel/preset-flow": "^7.0.0",
		"@babel/preset-react": "^7.0.0",
		"axios-mock-adapter": "^1.17.0",
		"jest-cucumber": "^2.0.11",
		"babel-plugin-typescript-to-proptypes": "^0.17.1",
		"enzyme": "^3.10.0",
		"enzyme-adapter-react-16": "^1.14.0",
		"enzyme-to-json": "^3.4.0",
		"file-loader": "^4.2.0",
		"html-webpack-plugin": "^3.2.0",
		"jest": "^24.9.0",
		"moxios": "^0.4.0",
		"react-test-renderer": "^16.9.0",
		"webpack": "^4.41.0",
		"webpack-cli": "^3.3.7",
		"webpack-dev-server": "^3.8.0"
	},
	"jest": {
		"transform": {
			"^.+\\.(jsx|js|jpg)$": "babel-jest"
		},
		"moduleFileExtensions": [
			"jsx",
			"js",
			"jpg"
		],
		"testMatch": [
			"<rootDir>/src/**/**/*.test.js",
			"<rootDir>/src/__tests__/*.js",
			"<rootDir>/src/**/**/*.test.jsx",
			"<rootDir>/src/__tests__/*.jsx"
		],
		"testPathIgnorePatterns": [
			"<rootDir>/node_modules"
		],
		"moduleNameMapper": {
			"\\.(css|scss|png|jpg)$": "identity-obj-proxy"
		},
		"setupFilesAfterEnv": [
			"<rootDir>/src/setupTests.js"
		]
	},
	"babel": {
		"presets": [
			"@babel/preset-env",
			"@babel/preset-react",
			"@babel/preset-flow"
		],
		"plugins": [
			"@babel/plugin-proposal-class-properties"
		]
	}
}
